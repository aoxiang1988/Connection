<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sec.myonlinefm.classificationprogram.fragment.ChannelProgramListFragment">

    <!-- TODO: Update blank fragment layout -->
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:layout_marginRight="@dimen/dialog_body_progress_margin_start"
        android:layout_marginLeft="@dimen/dialog_body_progress_margin_start"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/total_program"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:textSize="12sp"
            android:gravity="center"
            tools:ignore="SmallSp" />
        <TextView
            android:id="@+id/select"
            android:layout_gravity="right"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:textSize="12sp"
            android:gravity="center"
            android:clickable="true"
            android:focusable="true"
            android:layout_marginRight="50dp"
            tools:ignore="SmallSp" />
        <TextView
            android:id="@+id/order"
            android:layout_gravity="right"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:textSize="12sp"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            tools:ignore="SmallSp" />
    </FrameLayout>

    <com.sec.myonlinefm.defineview.RefreshListView
        android:id="@+id/program_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="24dp"
        android:layout_marginLeft="@dimen/dialog_body_progress_margin_start"
        android:layout_marginRight="@dimen/dialog_body_progress_margin_start"
        android:divider="@null" />

</FrameLayout>
