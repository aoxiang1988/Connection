<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/search_cover"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginLeft="@dimen/about_radio_badge_margin_start"
        app:srcCompat="@drawable/hybrid_radio_dns_img" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginRight="@dimen/about_radio_badge_margin_start"
        android:layout_gravity="center">

        <TextView
            android:id="@+id/search_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/search_freq"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:visibility="gone"/>

        <!--<TextView-->
            <!--android:id="@+id/search_cover"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:text="TextView" />-->
    </LinearLayout>
</LinearLayout>
